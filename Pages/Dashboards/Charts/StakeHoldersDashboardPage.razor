			<CarouselItem>
				<h5>Number of individuals reached with integrated health information campaigns to prevent violence (Overrall)</h5>
				<table style="width:100%;font-size:20px">
					<tr>
						<td style="border-color:ghostwhite; background-color:black; color:ghostwhite;">
							<strong>Stakeholder</strong>
						</td>

						<td style="border-color:ghostwhite; background-color:black; color:ghostwhite;">
							<strong>Q1</strong>
						</td>
						<td style="border-color:ghostwhite; background-color:black; color:ghostwhite;">
							<strong>Q2</strong>
						</td>
						<td style="border-color:ghostwhite; background-color:black; color:ghostwhite;">
							<strong>Q3</strong>
						</td>
						<td style="border-color:ghostwhite; background-color:black; color:ghostwhite;">
							<strong>Q4</strong>
						</td>
						<td style="border-color:ghostwhite; background-color:black; color:ghostwhite;">
							<strong>Annual</strong>
						</td>
					</tr>
					@{
						double? QuarterOne = 0;
						double? QuarterTwo = 0;
						double? QuarterThree = 0;
						double? QuarterFour = 0;
						double? Annual = 0;
						double? OverrallAnnual = 0;
					}
					@if (StakeholderdatalistOverrall != null)
					{
						@foreach (var item in StakeholderdatalistOverrall.Where(o=>o.IndicatorId==1))
						{
							<tr>
								<td style="background-color:ghostwhite; color:black;">
									@item.ServiceProviderDesc
								</td>

								<td style="text-align:right">	
									@item.QuarterOne.ToString("#,##0")
									@{
										QuarterOne += item.QuarterOne;
									}									
								</td>
								<td style="text-align:right">
									@item.QuarterTwo.ToString("#,##0")
									@{
										QuarterTwo += item.QuarterTwo;
									}
								</td>
								<td style="text-align:right">	
									@item.QuarterThree.ToString("#,##0")
									@{
										QuarterThree += item.QuarterThree;
									}									
								</td>
								<td style="text-align:right">
									@item.QuarterFour.ToString("#,##0")
									@{
										QuarterFour += item.QuarterFour;
									}									
								</td>
								<td style="text-align:right">
									@{
										Annual = @item.QuarterOne + @item.QuarterTwo + @item.QuarterThree + @item.QuarterFour;
										OverrallAnnual += Annual;
									}
										<strong>@Annual.Value.ToString("#,##0")</strong>
								</td>
							</tr>
						}
					}
					<tr>
						<td><strong>Total</strong></td>
						<td style ="text-align:right"><strong>@QuarterOne.Value.ToString("#,##0")</strong></td>
						<td style ="text-align:right"><strong>@QuarterTwo.Value.ToString("#,##0")</strong></td>
						<td style ="text-align:right"><strong>@QuarterThree.Value.ToString("#,##0")</strong></td>
						<td style ="text-align:right"><strong>@QuarterFour.Value.ToString("#,##0")</strong></td>			
						<td style ="text-align:right"><strong>@OverrallAnnual.Value.ToString("#,##0")</strong></td>
					</tr>
				</table>
